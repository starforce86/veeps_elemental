[pylint.MASTER]
ignore=migrations

[pylint.FORMAT]
disable = C0301, C0114, C0115, C0116, R0903, E1136, E1101, W0221, R0901
max-line-length = 150

[pylint.BASIC]
good-names=
    i,
    e,
    k,
    v,
    ex,
    t,
    template,
    logger

[pylint.LOGGING]
disable = W1203, C0209
logging-format-style=new
